@model ImageApp.Application.DTOs.Auth.LoginRequest

    <h2>Login</h2>

    <form method="post">
        <div>
            <label>Email</label>
            <input name="Email" type="email" required />
            </div>
        <div>
            <label>Password</label>
            <input name="Password" type="password" required />
        </div>
        <button type="submit">Login</button>
    </form>